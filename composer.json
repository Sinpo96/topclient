{
	"name" : "mofing/topclient",
	"type" : "library",
	"description" : "taobao sdk topclient with composer",
	"keywords" : [
		"taobao",
		"sdk",
		"topclient",
		"mofing"
	],
	"homepage" : "https://github.com/wcj343169893/topclient",
	"license" : "MIT",
	"authors" : [{
			"name" : "wenchaojun",
			"homepage" : "https://www.cjblog.org"
		}
	],
	"require" : {
		"php" : ">=5.6"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.5.*"
	},
	"autoload" : {
		"psr-4" : {
			"Taobao\\TopClient\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Taobao\\TopClient\\" : "tests"
		}
	},
	"prefer-stable" : true,
	"config" : {
		"sort-packages" : true
	}
}